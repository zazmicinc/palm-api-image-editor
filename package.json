{
  "name": "palm-api-image-editor",
  "displayName": "PaLM API Image Editor",
  "description": "Convert images to WebP format. Elevate your development process with efficient and integrated image editing, enhancing both your coding and design endeavors.",
  "version": "0.0.1",
  "publisher": "Zazmic",
  "license": "MIT",
  "repository": {
      "type": "git",
      "url": "https://github.com/zazmicinc/palm-api-image-editor.git"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#9e2b90",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Snippets",
    "Machine Learning",
    "Visualization"
  ],
  "keywords": [
    "image editor",
    "webp",
    "seo optimisation",
    "graphics",
    "palm api"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "palm-api-image-editor.ToWebP",
          "when": "resourceScheme == 'file' && resourceFilename =~ /\\.(png|jpg|jpeg|gif|bmp|ico)$/i",
          "title": "PaLM API: Convert Image to WebP",
          "group": "PaLM API"
        }
      ]
    },
    "commands": [
      {
        "command": "palm-api-image-editor.ToWebP",
        "title": "PaLM API: Convert Image to WebP"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
